{
  "printWidth": 100,
  "parser": "flow",
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "always",
  "jsxBracketSameLine": true,
  "overrides": [
    {
      "files": "*.scss",
      "options": {
        "parser": "scss"
      }
    },
    {
      "files": "*.json",
      "options": {
        "parser": "json"
      }
    }
  ]
}
